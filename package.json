{
    "name": "ripe-sdk",
    "version": "1.18.2",
    "description": "The public SDK for RIPE Core",
    "keywords": [
        "js",
        "ripe",
        "sdk"
    ],
    "homepage": "https://github.com/ripe-tech/ripe-sdk#readme",
    "bugs": {
        "url": "https://github.com/ripe-tech/ripe-sdk/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ripe-tech/ripe-sdk.git"
    },
    "license": "Apache-2.0",
    "author": {
        "name": "Platforme",
        "url": "https://www.platforme.com"
    },
    "main": "src/js/index.js",
    "unpkg": "dist/ripe.js",
    "files": [
        "gulpfile.js",
        "dist/**/*",
        "res/**/*",
        "src/css/**/*.css",
        "src/js/index.js",
        "src/js/ripe.js",
        "src/js/ripe-min.js",
        "src/js/api/**/*.js",
        "src/js/base/**/*.js",
        "src/js/locales/**/*.js",
        "src/js/plugins/**/*.js",
        "src/js/visual/**/*.js",
        "test/**/*.js"
    ],
    "scripts": {
        "build": "gulp build",
        "docs": "gulp docs",
        "gulp": "gulp",
        "lint": "gulp lint",
        "lint-fix": "gulp lint-fix",
        "mark": "gulp mark",
        "prettier": "prettier \"./*.{js,json}\" \"./{src,test}/**/*.{js,json}\" --write",
        "pretty": "npm run prettier && npm run lint-fix",
        "test": "gulp test",
        "upgrade": "npx sort-package-json && ncu -u",
        "watch": "gulp watch"
    },
    "dependencies": {
        "node-fetch": "^2.6.1",
        "xmlhttprequest": "^1.8.0"
    },
    "devDependencies": {
        "@babel/core": "^7.12.3",
        "@babel/polyfill": "^7.12.1",
        "@babel/preset-env": "^7.12.1",
        "eslint": "^7.12.1",
        "eslint-config-hive": "^0.3.6",
        "fancy-log": "^1.3.3",
        "gulp": "^4.0.2",
        "gulp-babel": "^8.0.0",
        "gulp-cli": "^2.3.0",
        "gulp-concat": "^2.6.1",
        "gulp-count": "^1.0.0",
        "gulp-eslint": "^6.0.0",
        "gulp-jsdoc3": "^3.0.0",
        "gulp-mocha": "^7.0.2",
        "gulp-replace": "^1.0.0",
        "gulp-size": "^3.0.0",
        "gulp-sourcemaps": "^2.6.5",
        "gulp-terser": "^1.4.1",
        "gulp-zip": "^5.0.2",
        "jsdom": "^16.4.0",
        "minami": "^1.2.3",
        "mocha": "^8.2.1",
        "mocha-cli": "^1.0.1",
        "npm-check-updates": "^9.2.4",
        "prettier": "^2.1.2",
        "prettier-config-hive": "^0.1.7",
        "sort-package-json": "^1.46.1",
        "uuid": "^8.3.1"
    }
}
